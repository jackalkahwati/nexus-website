{
  "name": "nexus-core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--no-warnings' next dev -p 3001",
    "dev:js": "NODE_OPTIONS='--no-warnings --max-old-space-size=4096' next dev",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "dev:debug": "NODE_OPTIONS='--inspect-brk' next dev",
    "build": "npm run build:types && next build",
    "build:types": "tsc --project tsconfig.build.json",
    "start": "next start",
    "start:prod": "NODE_ENV=production node server.js",
    "postinstall": "prisma generate",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix --max-warnings=0",
    "lint:fix": "npm run lint",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,yml,yaml,css,scss,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx,yml,yaml,css,scss,html}\"",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:watch": "npm run type-check -- --watch",
    "type-check:strict": "tsc --noEmit --strict",
    "validate": "npm run lint && npm run type-check && npm run format:check",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:ci": "npm run test:unit -- --ci --runInBand --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:api": "node test-api-server.js",
    "test:graph": "node test-graph-db.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force --skip-seed",
    "db:setup": "prisma migrate dev --name init && npm run db:seed",
    "setup": "node scripts/setup-env.js",
    "prepare": "husky install",
    "clean": "rimraf .next out dist coverage .cache .swc .turbo .vercel .docusaurus .nextflow .serverless .svelte-kit .svelte",
    "reset": "npm run clean && npm install && npm run db:reset",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "ANALYZE=server next build",
    "analyze:browser": "ANALYZE=browser next build",
    "size-limit": "size-limit",
    "size-limit:why": "size-limit --why",
    "check-updates": "npx npm-check-updates",
    "check-deps": "npx depcheck"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.9.0",
    "@radix-ui/react-accordion": "1.2.3",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-progress": "1.1.2",
    "@radix-ui/react-scroll-area": "1.2.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "1.1.8",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.13",
    "@stripe/react-stripe-js": "3.6.0",
    "@stripe/stripe-js": "2.4.0",
    "@tanstack/react-table": "8.21.2",
    "@typescript-eslint/typescript-estree": "8.29.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "5.47.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "1.1.1",
    "cors": "2.8.5",
    "date-fns": "^2.30.0",
    "diff": "7.0.0",
    "dotenv": "16.4.7",
    "elkjs": "0.10.0",
    "esbuild": "0.25.2",
    "express": "5.1.0",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.312.0",
    "mapbox-gl": "3.11.0",
    "neo4j-driver": "5.28.1",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "otplib": "^12.0.1",
    "react": "^18.2.0",
    "react-day-picker": "9.6.4",
    "react-dom": "^18.2.0",
    "react-dropzone": "14.3.8",
    "react-hook-form": "^7.50.1",
    "react-map-gl": "8.0.2",
    "reactflow": "11.11.4",
    "recharts": "^2.10.3",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.160.0",
    "uuid": "11.1.0",
    "winston": "3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "14.3.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/diff": "7.0.2",
    "@types/jest": "29.5.14",
    "@types/mapbox-gl": "^2.7.19",
    "@types/node": "20.17.47",
    "@types/react": "18.3.21",
    "@types/react-dom": "18.3.7",
    "@types/three": "^0.160.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-testing-library": "7.2.0",
    "eslint-plugin-unicorn": "59.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "15.5.2",
    "msw": "^1.3.2",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "postcss-nesting": "^12.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "pretty-quick": "^4.0.0",
    "prisma": "^5.9.0",
    "rimraf": "6.0.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.2",
    "typescript": "^5.3.3",
    "whatwg-fetch": "^3.6.17"
  },
  "engines": {
    "node": ">=18.x <=20.x"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bash -c 'npx tsc --noEmit --pretty --skipLibCheck' || true",
      "eslint --fix --max-warnings=0",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,md,mdx,yml,yaml,css,scss,html}": [
      "prettier --write --ignore-unknown"
    ],
    "**/*.{js,jsx,ts,tsx,json,md,mdx,yml,yaml,css,scss,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "singleAttributePerLine": false
  },
  "_trigger_deploy": "6"
}
